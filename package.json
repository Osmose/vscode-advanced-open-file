{
    "name": "vscode-advanced-open-file",
    "displayName": "vscode-advanced-open-file",
    "description": "",
    "version": "0.0.4",
    "publisher": "jit-y",
    "repository": {
        "type": "git",
        "url": "https://github.com/jit-y/vscode-advanced-open-file"
    },
    "engines": {
        "vscode": "^1.30.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.advancedOpenFile"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.advancedOpenFile",
                "title": "Advanced Open File"
            }
        ],
        "keybindings": [
            {
              "command": "extension.advancedOpenFile",
              "key": "ctrl+x ctrl+f"
            }
        ],
        "configuration":{
            "title": "VscodeAdvancedOpenFile Configuration",
            "properties": {
            }
        }
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "@types/glob": "^7.1.1",
        "@types/mocha": "^2.2.42",
        "@types/node": "^7.10.2",
        "typescript": "^2.6.1",
        "vscode": "^1.1.26"
    },
    "dependencies": {
        "glob": "^7.1.3"
    }
}
